{
  "name": "crypto-price-checker",
  "version": "0.0.1",
  "description": "A menubar app to check crypto prices",
  "main": "index.js",
  "scripts": {
    "start": "ENV=development electron .",
    "dev": "concurrently -k \"babel-node server.js\" \"npm start\"",
    "build": "webpack --config webpack.config.production.js && electron-packager . CryptoPriceChecker --platform=darwin --arch=all --prune --overwrite --icon ./bitcoin_icns.icns"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ciaran0/crypto-price-checker.git"
  },
  "keywords": [
    "Crypto",
    "Cryptocurrency",
    "Bitcoin",
    "Blockchain"
  ],
  "author": "Ciaran O'Connor",
  "license": "CC0-1.0",
  "homepage": "https://github.com/Ciaran0/crypto-price-checker",
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ]
  },
  "dependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.18.2",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "classnames": "^2.2.5",
    "electron-debug": "^1.1.0",
    "electron-prebuilt": "^0.36.0",
    "font-awesome": "^4.7.0",
    "lodash": "^4.17.4",
    "menubar": "^4.1.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux-persist": "^5.4.0",
    "resolve-url-loader": "^2.1.0"
  },
  "devDependencies": {
    "babel-loader": "^6.2.4",
    "concurrently": "^2.0.0",
    "css-loader": "^0.23.1",
    "debounce": "^1.0.0",
    "devtron": "^1.4.0",
    "electron-packager": "^6.0.0",
    "express": "^4.13.4",
    "file-loader": "^0.8.5",
    "moment": "^2.12.0",
    "postcss-loader": "^0.8.2",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-inlinesvg": "^0.7.4",
    "react-select": "^1.1.0",
    "redux": "^3.7.2",
    "redux-saga": "^0.9.5",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.9",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0",
    "webpack-target-electron-renderer": "^0.4.0"
  }
}
